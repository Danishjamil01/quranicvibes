        "id": 1,
        "chaptername": "Al-Fatihah",
        "arabicname": "الفَاتِحَةُ",
        "total_verses": 7,
        "bg_color": "#F4E3D6",
        "versename": "۞ سُورةُ الفاتِحَةِ مَكِيَّةٌ ۞",
        "allverse": [
            {
                "versenum": "1",
                "verse": "ٱلْحَمْدُ لِلَّٰهِ رَبِّ الْعَالَمِينَ",
                "versepart": [
                    {
                        "section": "الْحَمْدُ",
                        "partstarjuma": "تعریف یا حمد۔",
                        "partswajahat": "یہ لفظ حَمْد کی جذر سے آیا ہے جو کہ تعریف یا ستائش کا معنی رکھتا ہے۔ اس موقع پر یہ اللہ کی صفات اور اعمال کی تعریف اور شکرگزاری کا اظہار کرنے کے لئے استعمال ہو رہا ہے۔"
                    },
                    {
                        "section": "لِلّٰهِ",
                        "partstarjuma": "اللہ کے لیے",
                        "partswajahat": "یہ عبارت اللہ کے لئے سب تعریف اور شکر کو ظاہر کرتی ہے، اللہ کو عبادت اور تعریف کا موصول جانتے ہوئے۔"
                    },
                    {
                        "section": "رَبِّ",
                        "partstarjuma": "پروردگار",
                        "partswajahat": "رَبِّ ایک مکمل عربی لفظ ہے جو کہ پروردگار، رب، مالک، پروردگار اور رب بتایا کرتا ہے۔ یہ اللہ کی بنیادی خصوصیت کو ظاہر کرتا ہے کہ وہ سب کا مالک اور نگہبان ہے۔"
                    },
                    {
                        "section": "ٱلْعَالَمِينَ",
                        "partstarjuma": "جہانوں کا",
                        "partswajahat": "رَبِّ ایک مکمل عربی لفظ ہے جو کہ پروردگار، رب، مالک، پروردگار اور رب بتایا کرتا ہے۔ یہ اللہ کی بنیادی خصوصیت کو ظاہر کرتا ہے کہ وہ سب کا مالک اور نگہبان ہے۔"
                    },
                    {
                        "fulltarjuma": "سب تعریف  اللہ کے لیے ہے، جو سارے جہانوں کا رب ہے کہلایا جاتا ہے۔ یہ اللہ کے بارے میں اظہار شکر اور تعریف ہے، جو کہ تمام کائنات کا پروردگار اور مالک ہے۔"
                    }
                ]
            },
            {
                "versenum": "2",
                "verse": "الرَّحْمَـٰنِ الرَّحِيمِ",
                "versepart": [
                    {
                        "section": "الرَّحْمَـٰنِ",
                        "partstarjuma": "سب سے زیادہ رحم والا",
                        "partswajahat": "الرحمن اللہ کی انتہائی رحمت کو ظاہر کرتا ہے جو کہ تمام مخلوقات پر ہے، چاہے وہ مومن ہوں یا غیر مومن۔"
                    },
                    {
                        "section": "الرَّحِيمِ",
                        "partstarjuma": "سب سے زیادہ مہربان",
                        "partswajahat": "الرحیم  بھی اللہ کی خاص رحمت کو ظاہر کرتا ہے، خاص طور پر آخرت میں۔"
                    },
                    {
                        "fulltarjuma": "سب سے زیادہ رحم والا، سب سے زیادہ مہربان کا مطلب بنتا ہے۔ یہ اللہ کی رحمت اور مہربانی کی بات کرتا ہے، جو کہ عالمین کے لیے ہے۔"
                    }
                ]
            },
            {
                "versenum": "3",
                "verse": "مَالِكِ يَوْمِ الدِّينِ",
                "versepart": [
                    {
                        "section": "مَالِكِ",
                        "partstarjuma": "مالک یا حاکم۔",
                        "partswajahat": "مَالِكِ اللہ کو قیامت کے دن کا مالک یا حاکم ظاہر کرتا ہے، جس کا مطلب ہے کہ وہ اس دن کا تمام کچھ کنٹرول کرے گا اور اس دن کا فیصلہ کرے گا۔"
                    },
                    {
                        "section": "يَوْمِ الدِّينِ",
                        "partstarjuma": "قیامت کا دن۔",
                        "partswajahat": "يَوْمِ الدِّينِ قیامت کے دن کا مطلب ہے، جب سب کو اپنے اعمال کے بدلے میں اجر یا سزا دی جائے گی۔"
                    },
                    {
                        "fulltarjuma": "قیامت کے دن کا مالک یا قیامت کے دن کا حاکم کا مطلب ہے۔ یہ اللہ کو اس دن کا مالک اور حاکم ظاہر کرتا ہے جب سب انصاف کے لئے ان کے سامنے پیش کئے جائیں گے۔"
                    }
                ]
            },
            {
                "versenum": "4",
                "verse": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ",
                "versepart": [
                    {
                        "section": "إِيَّاكَ",
                        "partstarjuma": "تیرے ہی",
                        "partswajahat": "إِيَّاكَ اسلامی دعاؤں میں اللہ کو انعام یا مدد کی درخواست کے لئے استعمال ہوتا ہے۔"
                    },
                    {
                        "section": "نَعْبُدُ",
                        "partstarjuma": "عبادت کرتے ہیں",
                        "partswajahat": "نَعْبُدُ عبادت کا فعل ہے، جو کہ اللہ کی عبادت کے لئے استعمال ہوتا ہے۔"
                    },
                    {
                        "section": "وَإِيَّاكَ",
                        "partstarjuma": "اور تیرے ہی",
                        "partswajahat": "وَإِيَّاكَ اسلامی دعاؤں میں مدد یا انعام کی درخواست کے لئے استعمال ہوتا ہے۔"
                    },
                    {
                        "section": "نَسْتَعِينُ",
                        "partstarjuma": "مدد چاہتے ہیں۔",
                        "partswajahat": "نَسْتَعِينُ مدد یا انعام کی درخواست کرنے کا فعل ہے۔"
                    },
                    {
                        "fulltarjuma": "ہم تیری ہی عبادت کرتے ہیں اور تیری ہی مدد چاہتے ہیں۔"
                    }
                ]
            },
            {
                "versenum": "5",
                "verse": "اهْدِنَا الصِّرَاطَ الْمُسْتَقِيمَ",
                "versepart": [
                    {
                        "section": "اهْدِنَا",
                        "partstarjuma": "ہمیں راہ دکھا۔",
                        "partswajahat": "اهْدِنَا راہ دکھانے کا فعل ہے، جو کہ اللہ سے سیدھی راہ کی ہدایت کی درخواست کرتے وقت استعمال ہوتا ہے۔"
                    },
                    {
                        "section": "الصِّرَاطَ",
                        "partstarjuma": "راہ یا سڑک۔",
                        "partswajahat": "الصِّرَاطَ ایک عربی لفظ ہے جو کہ راہ یا سڑک کو ظاہر کرتا ہے۔"
                    },
                    {
                        "section": "الْمُسْتَقِيمَ",
                        "partstarjuma": "سیدھی",
                        "partswajahat": "الْمُسْتَقِيمَ سیدھی یا ٹھیک راہ کی بات کرتا ہے۔"
                    },
                    {
                        "fulltarjuma": "ہمیں سیدھی راہ دکھا کا مطلب ہے۔ یہ ایک دعا ہے جو مسلمان ہر نماز میں کہتے ہیں، جس میں اللہ سے سیدھی راہ کی ہدایت کی درخواست کی جاتی ہے۔"
                    }
                ]
            },
            {
                "versenum": "6",
                "verse": "صِرَاطَ الَّذِينَ أَنْعَمْتَ عَلَيْهِمْ غَيْرِ الْمَغْضُوبِ عَلَيْهِمْ وَلَاالضَّالِّينَ",
                "versepart": [
                    {
                        "section": "صِرَاطَ",
                        "partstarjuma": "راہ یا پٹھ",
                        "partswajahat": "یہاں 'صِرَاطَ' کا مطلب سیدھا راستہ یا پٹھ ہے، جو ہدایت اور کامیابی کا راستہ ہے۔"
                    },
                    {
                        "section": "الَّذِينَ",
                        "partstarjuma": "وہ لوگ",
                        "partswajahat": "یہاں 'الَّذِينَ' سے مراد وہ لوگ ہیں جو اللہ کی نعمتوں سے مستفید ہوئے ہیں۔"
                    },
                    {
                        "section": "أَنْعَمْتَ",
                        "partstarjuma": "تم نے نعمت بخشی ہے",
                        "partswajahat": "یہاں 'أَنْعَمْتَ' سے مراد اللہ کی طرف سے دی گئی نعمتیں ہیں۔"
                    },
                    {
                        "section": "عَلَيْهِمْ",
                        "partstarjuma": "ان پر",
                        "partswajahat": "یہاں 'عَلَيْهِمْ' سے مراد وہ لوگ ہیں جن پر اللہ کی نعمتیں نازل ہوئیں۔"
                    },
                    {
                        "section": "غَيْرِ",
                        "partstarjuma": "سواۓ",
                        "partswajahat": "یہاں 'غَيْرِ' کا مطلب اس کے علاوہ یا بغیر ہے۔"
                    },
                    {
                        "section": "الْمَغْضُوبِ",
                        "partstarjuma": "غضب انداز ہونے والے لوگ",
                        "partswajahat": "یہاں 'الْمَغْضُوبِ' سے مراد وہ لوگ ہیں جن پر اللہ کا غضب نازل ہوا۔"
                    },
                    {
                        "section": "عَلَيْهِمْ",
                        "partstarjuma": "ان پر",
                        "partswajahat": "یہاں 'عَلَيْهِمْ' دوبارہ سے ان لوگوں کی طرف اشارہ کرتا ہے جن پر اللہ کا غضب نازل ہوا۔"
                    },
                    {
                        "section": "وَلَا",
                        "partstarjuma": "اور نہ",
                        "partswajahat": "یہاں 'وَلَا' کا مطلب اور نہ یا اور نہیں ہے۔"
                    },
                    {
                        "section": "الضَّالِّينَ",
                        "partstarjuma": "گمراہ ہونے والے لوگ",
                        "partswajahat": "یہاں 'الضَّالِّينَ' سے مراد وہ لوگ ہیں جو گمراہ ہوگئے ہیں۔"
                    },
                    {
                        "fulltarjuma": "ان لوگوں کا راستہ جن پر تو نے انعام فرمایا، نہ ان کا جن پر تیرا غضب نازل ہوا اور نہ گمراہوں کا۔"
                    }
                ]
            }
        ]
